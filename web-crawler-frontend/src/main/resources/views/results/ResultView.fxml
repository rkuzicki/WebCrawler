<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<BorderPane xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pl.edu.agh.student.oop.webcrawler.frontend.views.results.presenter.ResultViewPresenter">
    <top>
        <TabPane tabClosingPolicy="UNAVAILABLE">
            <Tab text="%list_tab">
                <graphic>
                    <ImageView>
                        <Image url="/icons/list.png"/>
                    </ImageView>
                </graphic>
                <fx:include source="ResultList.fxml" fx:id="resultList"/>
            </Tab>
            <Tab text="%diagram_tab">
                <graphic>
                    <ImageView>
                        <Image url="/icons/diagram.png"/>
                    </ImageView>
                </graphic>
                <fx:include source="ResultDiagram.fxml"/>
            </Tab>
        </TabPane>
    </top>
</BorderPane>
